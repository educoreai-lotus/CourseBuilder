{
  "name": "course-builder-backend",
  "version": "1.0.0",
  "description": "Course Builder microservice backend - Express + PostgreSQL",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.js --runInBand --detectOpenHandles",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.js --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.js --coverage",
    "db:setup": "node scripts/setup-database.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "seed:mock": "node scripts/seedMockData.js",
    "db:reset": "npm run migrate && npm run seed",
    "db:init": "npm run db:setup && npm run migrate && npm run seed",
    "db:push-supabase": "node scripts/push-to-supabase.js",
    "db:create-test": "node scripts/create-test-db.js",
    "generate-secrets": "node scripts/generate-secrets.js"
  },
  "keywords": [
    "course-builder",
    "microservice",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@grpc/grpc-js": "^1.8.22",
    "@grpc/proto-loader": "^0.7.7",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "pg-promise": "^11.5.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
