{
  "name": "course-builder-backend",
  "version": "1.0.0",
  "description": "Course Builder microservice backend - Express + PostgreSQL",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.js --runInBand --detectOpenHandles",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.js --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.js --coverage",
    "db:setup": "node scripts/setup-database.js",
    "migrate": "node scripts/migrate.js",
    "migrate:team": "node scripts/migrateToDatabase.js",
    "migrate:custom": "node scripts/migrateToDatabase.js",
    "seed": "node scripts/seed.js",
    "seed:team": "node scripts/seedToDatabase.js",
    "seed:custom": "node scripts/seedToDatabase.js",
    "db:clear": "node scripts/clearDatabase.js",
    "db:init": "npm run db:setup && npm run migrate",
    "db:reset": "npm run db:clear && npm run migrate && npm run seed",
    "db:verify": "node scripts/verify-db.js",
    "db:diagnose": "node scripts/diagnose-db.js",
    "db:test-connection": "node scripts/test-connection.js",
    "check:ai-structure": "node scripts/check-ai-structure.js",
    "test:ai-course": "node test-api-course.js",
    "generate-secrets": "node scripts/generate-secrets.js",
    "generate-service-keys": "node scripts/generate-service-keys.js",
    "migrate:ai-assets": "node scripts/add-ai-assets-column.js",
    "register:coordinator": "node scripts/registerWithCoordinator.js"
  },
  "keywords": [
    "course-builder",
    "microservice",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@grpc/grpc-js": "^1.8.22",
    "@grpc/proto-loader": "^0.7.7",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.104.0",
    "pg-promise": "^11.5.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
